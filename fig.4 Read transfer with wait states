
{ signal: [
  { name: "PCLK",           wave: "p....." },
  { name: "PADDR",         wave: "2........x2..............................................",data: ["", "Addr 1"], period:0.1},
  { name: "PWRITE",         wave: "2.........0..............................................", period:0.1},
  { name: "PSEL",           wave: "0........x1.....................................x0......", period:0.1},
  { name: "PENABLE",        wave: "0...................x1.........................x0......", period:0.1},
  { name: "PRDATA",         wave: "2........x.............................2.......x2......",data:["","Data1"], period:0.1},
  { name: "PREADY",         wave: "2.......................0..................x1......2......",period:0.1, phase:1},
 ],
 head:{
   text:'Read transfer with wait states',
   tick:0,
 },
 foot:{
   text:'Figure 2-4',
   },
  config: { hscale: 3 }
